FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.000276;

boundaryField
{
    top
    {
        type            slip;
    }
    sides
    {
        type            slip;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.000276;
        value           uniform 0.000276;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.000276;
    }
    ground
    {
        type            slip;
    }
}


